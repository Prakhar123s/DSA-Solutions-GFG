/* Structure of node
class Node
{
    int data;
    Node next;
    Node(int d) {data = d; next = null; }
} */

class Solution {

    // Function to find the data of kth node from
    // the end of a linked list.
    int getKthFromLast(Node head, int k) {
        // Your code here
        
        Node temp=head;
        List<Integer> list=new ArrayList<>();
        
        while(temp!=null){
            
            list.add(temp.data);
            temp=temp.next;
        }
        
        Collections.reverse(list);
        
        if(k>list.size()){
            return -1;
        }
        
        return list.get(k-1);

    }
}
