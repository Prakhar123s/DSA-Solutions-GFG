class Solution {
    public static ArrayList<Integer> findUnion(int a[], int b[]) {
        // code here
        
        
        Set<Integer> s1=new TreeSet<>();
        
        for(int values:a){
            
            s1.add(values);
        }
        for(int values: b){
            s1.add(values);
        }
        
       return new ArrayList<>(s1);
        
        
    }
}
